{

  "DaprSidekick": {
    // Set the runtime location of config/components files to be the "dapr" folder under the deployed application
    "RuntimeDirectory": "dapr",
    "Sidecar": {
      "AppId": "backend",
      "AppSsl": false,
      "AppPort": 5001,
      "DaprHttpPort": 3501,
      "DaprGrpcPort": 50001
    },
    "Placement": {},
    "Sentry": {}
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*"
}
