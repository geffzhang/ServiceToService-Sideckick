{
  "DaprSidekick": {
    // Set the runtime location of config/components files to be the "dapr" folder under the deployed application
    "RuntimeDirectory": "dapr",
    "Sidecar": {
      "AppId": "proxy",
      "AppSsl": false,
      "AppPort": 6001,
      "DaprHttpPort": 3601,
      "DaprGrpcPort": 60001
    },
    "Placement": {},
    "Sentry": {}
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",

  "UseDaprInvoke": true
}
